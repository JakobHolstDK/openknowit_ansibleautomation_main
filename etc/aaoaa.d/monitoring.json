          "credential": "100_github",
          "master": "True"
        }
      ],
      "templates": [
        {
          "name": "111_checkup_zabbixclients",
          "description": "job checking connectivity",
          "job_type": "run",
          "inventory": "100_serverinventory",
          "project": "111_zabbixclients",
          "EE": "Automation Hub Default execution environment",
          "credentials": "100_servers",
          "playbook": "checkup.yml"
