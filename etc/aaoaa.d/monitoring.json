          "project": "120_databaseservers",
          "EE": "Automation Hub Default execution environment",
          "credentials": "120_databaseservers",
          "playbook": "checkup.yml"
        }
      ],
      "schedules": [
        {
          "name": "111_jobschedule_checkup_zabbixclients",
          "type": "job",
          "template": "111_checkup_zabbixclients",
          "description": "Master job for ensuring connectivity",
          "local_time_zone": "CET",
          "run_every_minute": "10",
